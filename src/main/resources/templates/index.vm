#set($layout = "/layout/blank.vm")
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <title>UA City</title>
    <meta name="description" content="">
    <meta name="author" content="Ted Huang">
    <link rel="stylesheet" href="#springUrl('/static/lib/weui.min.css')">
    <link rel="stylesheet" href="#springUrl('/static/css/jquery-weui.css')">
</head>
<body ontouchstart>
<div class="weui_search_bar" id="search_bar">
    <form class="weui_search_outer">
        <div class="weui_search_inner">
            <i class="weui_icon_search"></i>
            <input type="search" class="weui_search_input" id="search_input" placeholder="搜索" required/>
            <a href="javascript:" class="weui_icon_clear" id="search_clear"></a>
        </div>
        <label for="search_input" class="weui_search_text" id="search_text">
            <i class="weui_icon_search"></i>
            <span>搜索</span>
        </label>
    </form>
    <a href="javascript:" class="weui_search_cancel" id="search_cancel">取消</a>
</div>
<style>
    .weui-row {
        margin-top: 10px;
    }
    [class*="weui-col-"] {
        height: 105px;
        line-height: 20px;
        text-align: center;
    }
    .uac_grid_icon {
        width: 70px;
        height: 70px;
        margin: 0 auto;
    }
    .uac_grid_icon img {
        display: block;
        width: 100%;
        height: 100%;
    }

</style>

<div class="weui-row weui-no-gutter">
    <div class="weui-col-33">
        <a href="form.html"data-id="cell">

            <div class="uac_grid_icon">
                <img src="#springUrl('/static/images/icon/food.jpg')" alt="">
            </div>
            <p class="weui_grid_label">
                美食
            </p>

        </a>
    </div>
    <div class="weui-col-33">
        <a href="form.html" data-id="cell">
            <div class="uac_grid_icon">
                <img src="#springUrl('/static/images/icon/video.jpg')" alt="">
            </div>
            <p class="weui_grid_label">
                电影
            </p>
        </a>
    </div>
    <div class="weui-col-33">
        <a href="form.html" data-id="cell">
            <div class="uac_grid_icon">
                <img src="#springUrl('/static/images/icon/hotel.jpg')" alt="">
            </div>
            <p class="weui_grid_label">
                酒店
            </p>
        </a>
    </div>
</div>
<div class="weui-row weui-no-gutter">
    <div class="weui-col-33">
        <a href="form.html" data-id="cell">
            <div class="uac_grid_icon">
                <img src="#springUrl('/static/images/icon/market.jpg')" alt="">
            </div>
            <p class="weui_grid_label">
                购物
            </p>
        </a>
    </div>
    <div class="weui-col-33">
        <a href="form.html" data-id="cell">
            <div class="uac_grid_icon">
                <img src="#springUrl('/static/images/icon/scissors.jpg')" alt="">
            </div>
            <p class="weui_grid_label">
                美发
            </p>
        </a>
    </div>
    <div class="weui-col-33">
        <a href="form.html" data-id="cell">
            <div class="uac_grid_icon">
                <img src="#springUrl('/static/images/icon/pharmacy.jpg')" alt="">
            </div>
            <p class="weui_grid_label">
                药房
            </p>
        </a>
    </div>
</div>
<div class="weui-row weui-no-gutter">
    <div class="weui-col-33">大牌抢购</div>
    <div class="weui-col-33">外卖满减</div>
    <div class="weui-col-33">每日福利</div>
</div>

<div class="weui_tabbar">
    <a href="javascript:;" class="weui_tabbar_item weui_bar_item_on">
        <div class="weui_tabbar_icon">
            <img src="#springUrl('/static/images/icon/home.jpg')" alt="">
        </div>
        <p class="weui_tabbar_label">首页</p>
    </a>
    <a href="javascript:;" class="weui_tabbar_item">
        <div class="weui_tabbar_icon">
            <img src="#springUrl('/static/images/icon/store.jpg')" alt="">
        </div>
        <p class="weui_tabbar_label">商铺</p>
    </a>
    <a href="javascript:;" class="weui_tabbar_item">
        <div class="weui_tabbar_icon">
            <img src="#springUrl('/static/images/icon/car.jpg')" alt="">
        </div>
        <p class="weui_tabbar_label">服务</p>
    </a>
    <a href="javascript:;" class="weui_tabbar_item">
        <div class="weui_tabbar_icon">
            <img src="#springUrl('/static/images/icon/my.jpg')" alt="">
        </div>
        <p class="weui_tabbar_label">我的</p>
    </a>
</div>
</div>

</body>
<script src="#springUrl('/static/lib/jquery-2.1.4.js')"></script>
<script src="#springUrl('/static/js/jquery-weui.js')"></script>
</html>